<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Selector">
        <!-- custom attributes -->
        <attr name="selectorBackgroundColor" format="color" />
        <attr name="selectorTextAppearance" format="reference" />
        <attr name="selectorTitle" format="string" />

    </declare-styleable>

    <declare-styleable name="TextInput">
        <!-- custom attributes -->
        <attr name="TextInputSelectorbackgroundColor" format="color" />
        <attr name="TextInputbackgroundColor" format="color" />
        <attr name="textAppearance" format="reference" />
        <attr name="selectorDefaultIcon" format="integer" />
        <attr name="textSize" format="dimension" />

        <!-- standard attributes (android: prefix and no format attribute) -->
        <attr name="android:digits" />
        <attr name="android:inputType" />
        <attr name="android:selectAllOnFocus" />
        <attr name="android:hint" />
        <attr name="android:imeOptions" />
        <attr name="android:text" />

    </declare-styleable>

</resources>